package org.essoft

fun main() {
    val myMusicians = arrayListOf("Ertan", "Sidar", "Osman")
    println(myMusicians)

    val myMusicians2 = ArrayList<String>()
    myMusicians2.add("Ertan")
    myMusicians2.add("Sidar")
    myMusicians2.add("Osman")
    println(myMusicians2)
    myMusicians2.add(1, "Emre")
    println(myMusicians2)
}