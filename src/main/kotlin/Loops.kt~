package org.essoft

fun main() {

    val myArrayOfNumbers = arrayOf(1, 2, 3, 4, 5)
    // For Loop
    for (number in myArrayOfNumbers) {
        println(number)
    }

    for (i in 1..10) {
        println(i)
    }

    for (i in 1 until 10) {
        println(i)
    }

    for (i in 10 downTo 1) {
        println(i)
    }

    for (i in 1..10 step 2) {
        println(i)
    }
}